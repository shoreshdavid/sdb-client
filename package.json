    "react-big-calendar": "0.19.2",
    "react-dom": "16.4.2",
    "react-helmet": "5.2.0",
    "react-html-parser": "2.0.2",
    "react-refetch-component": "0.1.0",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "@types/enzyme": "3.1.13",
    "@types/jest": "23.3.1",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@types/react-router-dom": "4.3.0",
