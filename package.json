{
  "name": "shoreshdavidbrandon.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=src BROWSER=none PORT=8080 node scripts/start.js",
    "build": "NODE_PATH=src node scripts/build.js",
    "test": "NODE_PATH=src node scripts/test.js --env=jsdom",
    "deploy": "aws s3 cp build s3://shoreshdavidbrandon.com --recursive",
    "prettier:tsx": "./node_modules/.bin/prettier --write 'src/**/*.tsx'",
    "prettier:css": "./node_modules/.bin/prettier --write 'src/**/*.css'",
    "prettier:scss": "./node_modules/.bin/prettier --write 'src/**/*.scss'"
  },
  "dependencies": {
    "@types/reactstrap": "6.0.0",
    "apollo-boost": "0.1.6",
    "axios": "0.18.0",
    "bootstrap": "4.1.1",
    "dotenv": "4.0.0",
    "graphql": "0.13.2",
    "jest": "23.0.1",
    "moment": "2.22.1",
    "node-sass": "4.9.0",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "raven-js": "3.25.1",
    "react": "16.4.0",
    "react-apollo": "2.1.4",
    "react-dev-utils": "5.0.1",
    "react-dom": "16.4.0",
    "react-html-parser": "2.0.2",
    "react-router-dom": "4.2.2",
    "reactstrap": "6.1.0",
    "resolve": "1.6.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.10",
    "@types/jest": "22.2.3",
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "@types/react-router-dom": "4.2.6",
    "@types/react-test-renderer": "16.0.1",
    "@types/redux-logger": "3.0.6",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "enzyme": "3.3.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.12.1",
    "react-test-renderer": "16.3.2",
    "redux-devtools": "3.4.1",
    "sass-loader": "7.0.1",
    "source-map-loader": "0.2.3",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.4.2",
    "ts-loader": "3.2.0",
    "tslint": "5.9.1",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
