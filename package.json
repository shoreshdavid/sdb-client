{
  "name": "shoreshdavidbrandon.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=src BROWSER=none PORT=8080 node scripts/start.js",
    "build": "NODE_PATH=src node scripts/build.js",
    "test": "NODE_PATH=src node scripts/test.js --env=jsdom",
    "prettier:tsx": "npx prettier --write 'src/**/*.tsx'",
    "prettier:scss": "npx prettier --write 'src/**/*.scss'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.8",
    "@fortawesome/free-brands-svg-icons": "5.5.0",
